{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": [
            false
        ],
        "object-literal-sort-keys": false,
        "jsx-boolean-value": [
            "never"
        ],
        "jsx-no-lambda": true,
        "no-implicit-dependencies": false,
        "ordered-imports": [
            false
        ],
        "no-bitwise": false
    }
}