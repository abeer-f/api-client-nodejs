        "husky": "^0.13.4",
        "lint-staged": "^3.6.1",
        "mocha": "^3.0.0",
        "prettier": "^1.4.4",
        "ts-node": "^3.0.6",
        "tslint": "^5.4.3",
        "tslint-config-prettier": "^1.1.0",
        "typescript": "^2.3.2"
